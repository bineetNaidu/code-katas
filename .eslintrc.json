{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off",
        "no-return-assign": "off",
        "no-continue": "off",
        "eqeqeq": "warn"
    }
}
